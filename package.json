{
  "name": "@neoskop/hrbac",
  "version": "1.0.0-alpha.10",
  "description": "An implementation of a hierarchical role based access control for nodejs and browser.",
  "main": "dist/hrbac.bundle.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test:raw": "TS_NODE_CACHE=false mocha --compilers ts:ts-node/register 'src/**/*.spec.ts'",
    "test:watch": "npm run test:raw -- --watch --watch-extension ts",
    "test:coverage": "rimraf coverage && nyc -e '.ts' -r html -r text npm run test:raw",
    "test": "npm run test:coverage && nyc report -r text-lcov | coveralls",
    "clean": "rimraf lib dist index.d.ts index.js index.js.map index.metadata.json ng.d.ts ng.js ng.js.map ng.metadata.json",
    "prebuild": "npm run test:coverage",
    "build": "npm run clean && npm run build:ngc && npm run build:rollup:index && npm run build:rollup:ng",
    "build:ngc": "ngc -p tsconfig.esm.json",
    "build:rollup:index": "rollup -c rollup.index.js",
    "build:rollup:ng": "rollup -c rollup.ng.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoskop/hrbac.git"
  },
  "keywords": [
    "hierarchical",
    "rbac",
    "acl",
    "permission",
    "permissions",
    "access",
    "control",
    "role",
    "resource",
    "privilege",
    "assert"
  ],
  "author": "Mark Wecke <mark.wecke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoskop/hrbac/issues"
  },
  "homepage": "https://github.com/neoskop/hrbac#readme",
  "peerDependencies": {
    "@angular/common": "^4.3.6",
    "@angular/core": "^4.3.6",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/compiler-cli": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/platform-browser": "4.3.6",
    "@angular/router": "4.3.6",
    "@types/chai": "4.0.2",
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.20",
    "@types/sinon": "2.3.4",
    "@types/sinon-chai": "2.7.29",
    "chai": "4.1.1",
    "coveralls": "2.13.1",
    "mocha": "3.5.0",
    "nyc": "11.1.0",
    "rimraf": "2.6.1",
    "rollup": "0.50.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rxjs": "5.4.3",
    "sinon": "3.2.1",
    "sinon-chai": "2.13.0",
    "ts-node": "3.3.0",
    "tslib": "1.7.1",
    "typescript": "2.4.2",
    "zone.js": "0.8.17"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts",
      "lib",
      "lib.es6"
    ]
  },
  "dependencies": {}
}
