{
  "extends": "./tsconfig",
  "compilerOptions": {
    "outDir": "esm5",
    "target": "es5",
    "module": "es2015",
    "paths": {
      "@neoskop/hrbac": [ "public-api" ]
    }
  },
  "files": [
    "src/ng/public_api.ts"
  ],
  "angularCompilerOptions": {
    "flatModuleOutFile": "index.js",
    "flatModuleId": "@neoskop/hrbac/ng",
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true
  }
}
